{
  "name": "cijber/flea-market",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Cijber\\FleaMarket\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Cijber\\Tests\\FleaMarket\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "eater",
      "email": "=@eater.me"
    }
  ],
  "require": {
    "php": "^8",
    "ramsey/uuid": "4.1.1",
    "ext-sodium": "*",
    "mtdowling/jmespath.php": "2.6.1",
    "nikic/iter": "^2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "perftools/php-profiler": "0.18.0",
    "vimeo/psalm": "^4.8",
    "phpstan/phpstan": "^0.12.92",
    "jetbrains/phpstorm-attributes": "^1.0"
  },
  "suggest": {
    "ext-ffi": "Allows for atomic replacing of indexes and databases after compacting via libc renameat2 (cursed)"
  }
}
